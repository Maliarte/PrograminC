#include <stdio.h>
#include <stdlib.h>

void imprime(int *n, int m)
{ 
int i;
  for (i=0; i<m; i++) //m(5)
{
printf("%d n", n[i]);
}
}

int main (void) 
{
// declara vetor
int v[5]={1,2,3,4,5};
imprime(v,5); 
//Perceba: na chamada da fun√ß√£o apenas o nome do array(100 OP [],[i],[*]) 
//*Nu e cru como ele veio ao mundo.

return 0;
}
